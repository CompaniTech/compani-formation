    "expo-screen-orientation": "~4.2.0",
    "expo-sharing": "~10.2.0",
    "expo-splash-screen": "~0.15.1",
    "expo-system-ui": "~1.2.0",
    "expo-updates": "~0.13.4",
    "luxon": "^3.0.2",
