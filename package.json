{
  "scripts": {
    "prestart": "source $HOME/.nvm/nvm.sh; nvm use 12.13.0",
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "test": "node_modules/jest/bin/jest.js",
    "ts-lint": "tsc --noEmit && eslint ./src --ext .js,.jsx,.ts,.tsx -c .eslintrc.js",
    "eas-build-pre-install": "echo $IOS_GOOGLE_SERVICES_BASE64 | base64 --decode > ./GoogleService-Info.plist && echo $ANDROID_GOOGLE_SERVICES_DEV_BASE64_NEW | base64 --decode > ./google-services.json"
  },
  "jest": {
    "preset": "jest-expo",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.[jt]sx?$": "babel-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": [
      "\\.snap$",
      "<rootDir>/node_modules/"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|@react-native|react-clone-referenced-element|@react-native-community|@react-native-async-storage|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|@sentry/.*)"
    ],
    "cacheDirectory": ".jest/cache",
    "setupFiles": [
      "./mock.js"
    ]
  },
  "dependencies": {
    "@formatjs/intl-datetimeformat": "^5.0.1",
    "@formatjs/intl-getcanonicallocales": "^1.9.2",
    "@formatjs/intl-locale": "^2.4.47",
    "@formatjs/intl-numberformat": "^7.4.3",
    "@formatjs/intl-pluralrules": "^4.3.3",
    "@react-native-async-storage/async-storage": "~1.17.3",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-native-community/slider": "4.2.1",
    "@react-navigation/bottom-tabs": "^6.3.1",
    "@react-navigation/material-top-tabs": "^6.2.1",
    "@react-navigation/native": "^6.0.10",
    "@react-navigation/stack": "^6.2.1",
    "array-flat-polyfill": "^1.0.1",
    "axios": "^0.27.2",
    "babel-jest": "^27.5.1",
    "bignumber.js": "^9.0.2",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.1.4",
    "expo": "^45.0.0",
    "expo-application": "~4.1.0",
    "expo-asset": "~8.5.0",
    "expo-av": "~11.2.3",
