    "@sentry/react-native": "5.19.1",
    "array-flat-polyfill": "^1.0.1",
    "axios": "^1.6.7",
    "babel-jest": "^29.7.0",
    "bignumber.js": "^9.0.2",
    "eslint-config-airbnb-base": "^15.0.0",
