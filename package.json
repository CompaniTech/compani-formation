    "expo-image-manipulator": "~10.2.0",
    "expo-image-picker": "~12.0.1",
    "expo-intent-launcher": "~10.1.3",
    "expo-linear-gradient": "~11.0.3",
    "expo-notifications": "~0.14.0",
    "expo-screen-orientation": "~4.1.1",
    "expo-sharing": "~10.1.0",
    "expo-updates": "~0.11.7",
    "luxon": "^2.3.2",
    "mime": "^3.0.0",
    "qs": "^6.10.3",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-native": "0.64.3",
    "react-native-draggable-flatlist": "^3.1.2",
