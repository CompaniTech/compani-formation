{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "prestart": "source $HOME/.nvm/nvm.sh; nvm use 12.13.0",
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "test": "node_modules/jest-expo/node_modules/jest/bin/jest.js",
    "ts-lint": "tsc --noEmit & eslint ./src --ext .js,.jsx,.ts,.tsx -c .eslintrc.js",
    "lint": "eslint ./src --ext .js,.jsx,.ts,.tsx -c .eslintrc.js"
  },
  "jest": {
    "preset": "jest-expo",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.[jt]sx?$": "babel-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": [
      "\\.snap$",
      "<rootDir>/node_modules/"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-clone-referenced-element|@react-native-community|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|@sentry/.*)"
    ],
    "cacheDirectory": ".jest/cache",
    "setupFiles": [
      "./mock.js"
    ]
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.12.1",
    "@react-native-community/masked-view": "0.1.10",
    "@react-native-community/slider": "3.0.3",
    "@react-navigation/bottom-tabs": "^5.11.2",
    "@react-navigation/material-top-tabs": "^5.3.10",
    "@react-navigation/native": "^5.8.10",
    "@react-navigation/stack": "^5.12.8",
    "array-flat-polyfill": "^1.0.1",
    "axios": "^0.19.2",
    "babel-jest": "^26.6.3",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.18.0",
    "expo": "^39.0.4",
    "expo-av": "~8.6.0",
    "expo-font": "~8.3.0",
    "expo-linear-gradient": "~8.3.0",
    "expo-screen-orientation": "~2.0.0",
    "moment": "^2.29.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-39.0.0.tar.gz",
    "react-native-draggable-flatlist": "^2.5.0",
    "react-native-drax": "^0.7.2",
    "react-native-gesture-handler": "~1.7.0",
    "react-native-reanimated": "^1.13.2",
    "react-native-safe-area-context": "3.1.4",
    "react-native-screens": "~2.10.1",
    "react-native-svg": "12.1.0",
    "react-native-svg-charts": "^5.4.0",
    "react-native-tab-view": "^2.15.2",
    "react-redux": "^7.2.2",
    "react-test-renderer": "~16.11.0",
    "redux": "^4.0.5",
    "sharp-cli": "^1.14.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.7",
    "@types/expo": "^33.0.1",
    "@types/jest": "^26.0.15",
    "@types/react": "^16.14.1",
    "@types/react-dom": "^16.9.10",
    "@types/react-native": "^0.63.36",
    "@types/react-test-renderer": "^16.9.4",
    "@typescript-eslint/eslint-plugin": "^3.9.0",
    "@typescript-eslint/parser": "^3.9.0",
    "axios-mock-adapter": "^1.19.0",
    "babel-preset-expo": "^8.3.0",
    "eslint": "^7.14.0",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^26.6.3",
    "jest-expo": "^39.0.0",
    "react-native-testing-library": "^2.1.0",
    "reactotron-react-native": "^5.0.0",
    "reactotron-redux": "^3.1.3",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.2"
  },
  "private": true
}
