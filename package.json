    "@types/jest": "^27.4.1",
    "@types/luxon": "^2.3.1",
    "@types/react": "^17.0.47",
    "@types/react-dom": "~17.0.9",
    "@types/react-native": "~0.64.12",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
